class ArrayBub
{
	private long[] a;
	private int nElems;
	public ArrayBub(int max)
	{
		a = new long[max];
		nElems = 0;
	}
	public void insert(long value)
	{
		a[nElems] = value;	
		nElems++;
	}
	public void display()
	{
		for(int j=0; j<nElems; j++)
		System.out.print(a[j] + " ");
		System.out.println("");
	}
	public void bubbleSort ()
	{
		int out, in,ctr=1;
		for(out=nElems; out>1; out--)
		{
			for(in=0; in<out; in++)
				if( a[in] > a[in+1] )
				{
					swap(in, in+1);
					System.out.println(" SWAP NO. " +ctr);
					System.out.println("");
					display();
					ctr++;
				}
		}
	}
	private void swap(int one, int two)
	{
		long temp = a[one];
		a[one] = a[two];
		a[two] = temp;
	}
} 
public class prob2
{
	public static void main(String[] args)
	{
		int maxSize = 100;
		ArrayBub arr;
		
		arr = new ArrayBub(maxSize);
		System.out.println("");
		System.out.println("*_______USING BUBBLE SORT_______*");
		System.out.println("");
		arr.insert(99);
		arr.insert(39);
		arr.insert(19);
		arr.insert(49);
		arr.insert(89);
		arr.insert(79);
		arr.insert(29);
		arr.insert(99);
		arr.insert(59);
		arr.insert(9);
		arr.bubbleSort();
		arr.display();
	}
}
